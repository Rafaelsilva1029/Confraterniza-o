# ============================================
# BANCO DE DADOS - SUPABASE
# ============================================
# URL de conexão do banco de dados Supabase
# Formato: mysql://user:password@host:port/database
DATABASE_URL=mysql://user:password@db.supabase.co:3306/postgres

# ============================================
# AUTENTICAÇÃO - OAUTH MANUS
# ============================================
# ID da aplicação OAuth Manus
VITE_APP_ID=seu-app-id-manus

# URL do servidor OAuth Manus
OAUTH_SERVER_URL=https://api.manus.im

# URL do portal OAuth Manus para login (frontend)
VITE_OAUTH_PORTAL_URL=https://oauth.manus.im

# Chave secreta para assinar tokens JWT de sessão
JWT_SECRET=sua-chave-secreta-super-segura-aqui

# ============================================
# INFORMAÇÕES DO PROPRIETÁRIO
# ============================================
# Nome do proprietário da aplicação
OWNER_NAME=Seu Nome

# ID aberto do proprietário (OpenID)
OWNER_OPEN_ID=seu-open-id-manus

# ============================================
# API FORGE MANUS (Backend)
# ============================================
# URL da API Forge interna (servidor)
BUILT_IN_FORGE_API_URL=https://api.manus.im

# Chave de API Forge interna (servidor)
BUILT_IN_FORGE_API_KEY=sua-chave-api-forge-backend

# ============================================
# API FORGE MANUS (Frontend)
# ============================================
# URL da API Forge para frontend
VITE_FRONTEND_FORGE_API_URL=https://api.manus.im

# Chave de API Forge para frontend
VITE_FRONTEND_FORGE_API_KEY=sua-chave-api-forge-frontend

# ============================================
# CONFIGURAÇÃO DA APLICAÇÃO
# ============================================
# Título da aplicação (exibido no navegador)
VITE_APP_TITLE=Aplicativo de Confraternização

# URL do logo da aplicação
VITE_APP_LOGO=https://seu-dominio.com/logo.png

# ============================================
# ANALYTICS
# ============================================
# Endpoint de analytics
VITE_ANALYTICS_ENDPOINT=https://analytics.manus.im

# ID do website para analytics
VITE_ANALYTICS_WEBSITE_ID=seu-website-id

# ============================================
# NODE ENVIRONMENT
# ============================================
# Ambiente de execução (development, production, test)
NODE_ENV=production
